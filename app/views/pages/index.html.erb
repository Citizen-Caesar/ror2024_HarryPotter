<h1>Personajes de la película Harry Potter</h1>
<p>Casa de magia y su ubicación</p>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Location</th>
      <th scope="col">House</th>
    </tr>
  </thead>
  <tbody>
        <%= @character.each do |c| %>
            <tr>
                <td><%= c.id  %></td>
                <td><%= c.name  %></td>
                <td><%= c.location  %></td>
                <td><%= c.house  %></td>
            </tr>
        <% end %>
  </tbody>
</table>